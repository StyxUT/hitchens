<h1>Listing virus_characteristics</h1>

<table>
  <tr>
    <th>Virus</th>
    <th>Characteristic</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @virus_characteristics.each do |virus_characteristic| %>
  <tr>
    <td><%= virus_characteristic.virus_id %></td>
    <td><%= virus_characteristic.characteristic_id %></td>
    <td><%= virus_characteristic.value %></td>
    <td><%= link_to 'Show', virus_characteristic %></td>
    <td><%= link_to 'Edit', edit_virus_characteristic_path(virus_characteristic) %></td>
    <td><%= link_to 'Destroy', virus_characteristic, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

